{
  "kind": "collectionType",
  "collectionName": "travel_cards",
  "info": {
    "name": "UmrahPackages",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "label": {
      "type": "string"
    },
    "destinations": {
      "type": "string"
    },
    "price": {
      "type": "component",
      "repeatable": true,
      "component": "currency.price"
    },
    "pricingNote": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "packageDateRange": {
      "type": "json"
    },
    "hotelTypes": {
      "type": "component",
      "repeatable": true,
      "component": "options.hotel-options",
      "required": true
    },
    "packages": {
      "type": "component",
      "repeatable": false,
      "component": "options.package-options",
      "required": true
    },
    "tripTypes": {
      "type": "component",
      "repeatable": true,
      "component": "options.trip-options",
      "required": true
    },
    "flightDetails": {
      "type": "component",
      "repeatable": true,
      "component": "options.flight-component"
    },
    "activityDetails": {
      "type": "component",
      "repeatable": true,
      "component": "options.activity-component"
    },
    "transferDetails": {
      "type": "component",
      "repeatable": true,
      "component": "options.transfer-component"
    },
    "cities": {
      "type": "component",
      "repeatable": true,
      "component": "options.city-options",
      "required": false
    },
    "insurance": {
      "type": "component",
      "repeatable": true,
      "component": "options.insurances"
    },
    "DepositPrice": {
      "type": "integer",
      "required": true
    },
    "InfantPrice": {
      "type": "integer"
    },
    "NeedInsurance": {
      "type": "boolean"
    }
  }
}
