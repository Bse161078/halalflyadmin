{
  "kind": "collectionType",
  "collectionName": "travel_cards",
  "info": {
    "name": "TravelCard",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "label": {
      "type": "string"
    },
    "destinations": {
      "type": "string"
    },
    "price": {
      "type": "component",
      "repeatable": true,
      "component": "currency.price"
    },
    "pricingNote": {
      "type": "string"
    },
    "flightIncluded": {
      "type": "boolean",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "packageDateRange": {
      "type": "json"
    },
    "city": {
      "type": "component",
      "repeatable": true,
      "component": "options.city-options"
    },
    "hotelTypes": {
      "type": "component",
      "repeatable": true,
      "component": "options.hotel-options"
    },
    "packages": {
      "type": "component",
      "repeatable": true,
      "component": "options.package-options"
    },
    "roomTypes": {
      "type": "component",
      "repeatable": true,
      "component": "options.room-options"
    },
    "tripTypes": {
      "type": "component",
      "repeatable": true,
      "component": "options.trip-options"
    }
  }
}
